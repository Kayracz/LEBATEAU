<%= form_for [@boat, @booking] do |f| %>
<% if @booking.errors.any? %>
  <% @booking.errors.full_messages.each do |message| %>
    <%= message %>
  <% end %>
<% end %>
<%= f.date_field :start_date, label: 'Start date' %>
<%= f.date_field :end_date, label: 'End date' %>
<%= f.submit :submit %>
<% end %>
